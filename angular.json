{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "ng-siac": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                },
                "@schematics/angular:application": {
                    "strict": true
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:application",
                    "options": {
                        "outputPath": {
                          "base": "dist/ng-siac"
                        },
                        "index": "src/index.html",
                        "polyfills": [
                          "src/polyfills.ts"
                        ],
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            "src/manifest.webmanifest",
                            "src/web.config",
                            {
                                "glob": "**/*",
                                "input": "./node_modules/@kolkov/angular-editor/assets/",
                                "output": "./assets/fonts/"
                            },
                            {
                                "glob": "**/*",
                                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                                "output": "/assets/"
                            }
                        ],
                        "styles": [
                            "./node_modules/bootstrap/dist/css/bootstrap.min.css",
                            "./node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
                            "src/styles/styles.scss",
                            "./node_modules/bootstrap/dist/css/bootstrap-grid.css",
                            "./node_modules/quill/dist/quill.core.css",
                            "./node_modules/quill/dist/quill.bubble.css",
                            "./node_modules/quill/dist/quill.snow.css"
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "src/styles"
                            ]
                        },
                        "scripts": [
                            "node_modules/quill/dist/quill.min.js",
                            "node_modules/quill-blot-formatter/dist/quill-blot-formatter.min.js",
                            "node_modules/jquery/dist/jquery.js",
                            "node_modules/bootstrap-maxlength/dist/bootstrap-maxlength.js",
                            "node_modules/jquery/dist/jquery.min.js",
                            "node_modules/@popperjs/core/dist/umd/popper.min.js",
                            "node_modules/bootstrap/dist/js/bootstrap.min.js"
                        ],
                        "serviceWorker": "ngsw-config.json",
                        "allowedCommonJsDependencies": [
                            "pdfjs-dist/build/pdf",
                            "pdfjs-dist/web/pdf_viewer",
                            "quill-blot-formatter",
                            "sweetalert2",
                            "quill",
                            "moment",
                            "file-saver"
                        ],
                        "browser": "src/main.ts"
                    },
                    "defaultConfiguration": "development",
                    "configurations": {
                        "development": {
                          "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true,
                            "outputPath": {
                              "base": "dist/dev"
                            }
                        },
                        "dev": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "5mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "20kb",
                                    "maximumError": "20kb"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.dev.ts"
                                },
                                {
                                    "replace": "src/app/utils/enums/modules-idV2.ts",
                                    "with": "src/app/utils/enums/modules-idV2.dev.ts"
                                }
                            ],
                            "outputHashing": "all",
                            "outputPath": {
                              "base": "dist/dev"
                            }
                        },
                        "qa": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "5mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "20kb",
                                    "maximumError": "20kb"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.qa.ts"
                                },
                                {
                                    "replace": "src/app/utils/enums/modules-idV2.ts",
                                    "with": "src/app/utils/enums/modules-idV2.qa.ts"
                                }
                            ],
                            "outputHashing": "all",
                            "outputPath": {
                              "base": "dist/qa"
                            }
                        },
                        "prod": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "5mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "20kb",
                                    "maximumError": "20kb"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                },
                                {
                                    "replace": "src/app/utils/enums/modules-idV2.ts",
                                    "with": "src/app/utils/enums/modules-idV2.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "namedChunks": false,
                            "outputHashing": "all",
                            "outputPath": {
                              "base": "dist/prod"
                            }
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "defaultConfiguration": "development",
                    "configurations": {
                        "development": {
                          "buildTarget": "ng-siac:build"
                        },
                        "dev": {
                          "buildTarget": "ng-siac:build:dev"
                        },
                        "qa": {
                          "buildTarget": "ng-siac:build:qa"
                        },
                        "prod": {
                          "buildTarget": "ng-siac:build:prod"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                      "buildTarget": "ng-siac:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            "src/manifest.webmanifest",
                            "src/web.config",
                            {
                                "glob": "**/*",
                                "input": "./node_modules/@kolkov/angular-editor/assets/",
                                "output": "./assets/fonts/"
                            }
                        ],
                        "styles": [
                            "./node_modules/bootstrap/dist/css/bootstrap.min.css",
                            "./node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
                            "src/styles/styles.scss",
                            "./node_modules/bootstrap/dist/css/bootstrap-grid.css",
                            "./node_modules/quill/dist/quill.core.css",
                            "./node_modules/quill/dist/quill.bubble.css",
                            "./node_modules/quill/dist/quill.snow.css"
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "src/styles"
                            ]
                        },
                        "scripts": [
                            "./node_modules/quill/dist/quill.min.js",
                            "./node_modules/quill-blot-formatter/dist/quill-blot-formatter.min.js",
                            "./node_modules/jquery/dist/jquery.min.js",
                            "./node_modules/@popperjs/core/dist/umd/popper.min.js",
                            "./node_modules/bootstrap/dist/js/bootstrap.min.js"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "src/**/*.ts",
                            "src/**/*.html"
                        ]
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": "69cfecc9-af5b-432a-898a-385023e0f4e8"
    }
}